{
  "name": "gamefind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "lodash.intersectionby": "^4.7.0",
    "lodash.samplesize": "^4.2.0",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.0",
